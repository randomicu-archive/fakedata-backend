Project FakedataBackend {
database_type: 'PostgreSQL'
note: '''
# Randomicu Fakedata Database

Statistics from Fakedata backend
'''
}

Enum event_type {
  address [note: 'event from Address route']
  person [note: 'event from Person route']
}

Table event as S {
  event_id int [pk, increment]
  created_at datetime [not null, default: `now()`]
  language varchar(255) [not null]
  event_type event_type [not null]

note: "table 'event' contains information about events from routes"
}
